{
    "applications": {
        "gecko": {
            "id": "quicklaunch@mozillaonline.com",
            "strict_min_version": "56.0"
        }
    },
    "default_locale": "zh_CN",
    "description": "__MSG_extensionDescription__",
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16" :"images/icons/browser_action-icon-16x16.png",
            "32" :"images/icons/browser_action-icon-32x32.png"

        },
        "default_popup": "popup/popup.html",
        "default_title": "__MSG_browserActionTitle__"
    },
    "permissions" :[
        "nativeMessaging"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "homepage_url": "http://www.firefox.com.cn/",
    "icons": {
        "32": "images/icons/icon-32x32.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "version": "1.1.8"
}