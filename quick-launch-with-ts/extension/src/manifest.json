{
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16" :"images/icons/browser_action-icon-16x16.png",
            "32" :"images/icons/browser_action-icon-32x32.png"
        },
        "default_popup": "popup/popup.html",
        "default_title": "__MSG_browserActionTitle__"
    },
    "options_ui": {
        "page": "options/options.html",
        "browser_style": true
    },
    "default_locale": "en_US",
    "description": "__MSG_extensionDescription__",
    "homepage_url": "http://www.firefox.com.cn/",
    "icons": {
        "32": "images/icons/icon-32x32.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "permissions" :[
        "nativeMessaging",
        "storage",
        "notifications",
        "<all_urls>"
    ],
    "applications": {
        "gecko": {
            "id": "quicklaunch@mozillaonline.com",
            "strict_min_version": "57.0"
        }
    },
    "version": "1.1.8"
}