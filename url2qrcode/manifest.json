{
    "applications": {
        "gecko": {
            "id": "url2qrcode@mozillaonline.com",
            "strict_min_version": "57.0"
        }
    },
    "background": {
        "scripts": [
            "url2qr.background.js"
        ]
    },
    "default_locale": "en_US",
    "description": "__MSG_extensionDescription__",
    "homepage_url": "http://www.firefox.com.cn/",
    "icons": {
        "32": "/images/url2qrcode-page-icon.png"
    },
    "manifest_version": 2,
    "name": "__MSG_extensionName__",
    "page_action": {
        "browser_style": true,
        "default_icon": "/images/url2qrcode-page-icon.png",
        "default_popup": "/popup/url2qrcode-popup.html",
        "default_title": "__MSG_popupTitle__"
    },
    "permissions": [
        "tabs"
    ],
    "version": "0.0.3"
}